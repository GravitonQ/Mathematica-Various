 TemplateBox[<|"boxes" -> FormBox[
     RowBox[{
       StyleBox["T", "TI"], 
       RowBox[{"(", 
         RowBox[{
           StyleBox["r", "TI"], ",", 
           StyleBox["t", "TI"]}], ")"}], "\[LongEqual]", 
       SubscriptBox[
        StyleBox["T", "TI"], "1"], "+", 
       FractionBox[
        RowBox[{
          StyleBox["B", "TI"], "(", 
          RowBox[{
            StyleBox["r", "TI"], ",", 
            StyleBox["t", "TI"]}], ")"}], 
        StyleBox["r", "TI"]]}], TraditionalForm], "errors" -> {}, "input" -> 
   "T\\left(r,t\\right)=T_{1}+\\frac{B\\left(r,t\\right)}{r}", "state" -> 
   "Boxes"|>,
  "TeXAssistantTemplate"]], "Input",
 CellChangeTimes->{
  3.873470232981886*^9},ExpressionUUID->"e5fb6998-c198-42a5-b7b1-\
920374df25e2"],

